{
  "name": "Nomand",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --sequential webpack-dev",
    "build": "npm-run-all --sequential webpack-build",
    "webpack-dev": "better-npm-run webpack-dev-server",
    "webpack-build": "better-npm-run webpack-build"
  },
  "betterScripts": {
    "webpack-dev-server": {
      "command": "webpack-dev-server -colors --devtool --hot --host 0.0.0.0 --port:8080  --content-base ./src --config ./build/webpack.dev.config.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": ""
      }
    },
    "webpack-build": {
      "command": "webpack -colors --progress --profile --display-error-details --hide-modules --content-base ./src --config ./build/webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": ""
      }
    }
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "history": "^2.0.1",
    "react": "^15.0.1",
    "stylus": "^0.54.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "axios": "^0.9.1",
    "babel": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.8",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "immutable": "^3.7.6",
    "jsx-loader": "^0.13.2",
    "npm-run-all": "^1.7.0",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.1.1",
    "react-router-redux": "^4.0.2",
    "redux": "^3.4.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.10",
    "redux-form": "^5.0.1",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "style-loader": "^0.13.1",
    "stylus-loader": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "author": "Nomand",
  "license": "MIT"
}
